<service>
    <id>TungstenAgent</id>
    <name>TungstenAgent</name>
    <description>Tungsten vRouter Agent</description>
    <executable>tungsten-vrouter-agent.exe</executable>
    <arguments>--config_file=%ALLUSERSPROFILE%\Tungsten\etc\tungsten\tungsten-vrouter-agent.conf</arguments>
    <logpath>%ALLUSERSPROFILE%\Tungsten\var\log\tungsten</logpath>
    <logmode>append</logmode>
</service>
