import os

Import('CfgmEnv')
env = CfgmEnv.Clone()

gobin = env.GoCniBuild('mesos_cni')

env.Alias('tungsten-mesos-cni', gobin)
env.Alias('install', env.InstallAs(env['INSTALL_BIN'] + '/tungsten-mesos-cni', gobin))
env.Default(gobin)
